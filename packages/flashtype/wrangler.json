{
	"$schema": "https://unpkg.com/wrangler@latest/config-schema.json",
	"name": "flashtype",
	"main": "worker.ts",
	"compatibility_date": "2025-09-15",
	"assets": {
		"binding": "ASSETS",
		"directory": "dist"
	},
	"build": {
		"cwd": "../..",
		"command": "pnpm --filter @opral/flashtype... build"
	},
	"routes": [
		{
			"pattern": "flashtype.ai",
			"custom_domain": true
		}
	],
	"preview_urls": true
}
